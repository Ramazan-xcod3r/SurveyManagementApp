@model dynamic

@{
    ViewBag.Title = "title";
    Layout = "~/Areas/Manager/Views/Shared/_ManagerLayout.cshtml";
}
@{
    int c = 0;
    var perList = ViewBag.perList;
    var SurveyList = ViewBag.SurveyList;
    var CompanyList=ViewBag.CompanyList;
    var SolvedSurveyAnswerList=ViewBag.SolvedSurveyAnswerList;
    int perCount = 0;
}
<h2>DASHBOARD</h2>
<br/>
   <!--state overview start-->
              <div class="row state-overview">
                  <div class="col-lg-3 col-sm-6">
                      <section class="panel">
                          <div class="symbol terques">
                              <i class="fa fa-user"></i>
                          </div>
                          <div class="value">
                              <h1 class="count_1">@perList</h1>
                              <p>Users</p>
                          </div>
                      </section>
                  </div>
                  <div class="col-lg-3 col-sm-6">
                      <section class="panel">
                          <div class="symbol red">
                              <i class="fa fa-compass"></i>
                          </div>
                          <div class="value">
                              <h1 class="count_2">@CompanyList</h1>
                              <p>Companies</p>
                          </div>
                      </section>
                  </div>
                  <div class="col-lg-3 col-sm-6">
                      <section class="panel">
                          <div class="symbol yellow">
                              <i class="fa fa-superscript"></i>
                          </div>
                          <div class="value">
                              <h1 class="count_3">@SurveyList</h1>
                              <p>Surveys</p>
                          </div>
                      </section>
                  </div>
                  <div class="col-lg-3 col-sm-6">
                      <section class="panel">
                          <div class="symbol blue">
                              <i class="fa fa-bar-chart-o"></i>
                          </div>
                          <div class="value">
                              <h1 class=" count_4">@SolvedSurveyAnswerList</h1>
                              <p>Solved Survey Questions</p>
                          </div>
                      </section>
                  </div>
              </div>
              <!--state overview end-->
              <div class="row">
                  <div class="col-lg-4">
                      <!--user info table start-->
                      <section class="panel">
                          <div class="panel-body">
                              <a href="index.html#" class="task-thumb">
                                  <img src="img/avatar1.jpg" alt="">
                              </a>
                              <div class="task-thumb-details">
                                  <h1><a href="index.html#">Anjelina Joli</a></h1>
                                  <p>Senior Architect</p>
                              </div>
                          </div>
                          <table class="table table-hover personal-task">
                              <tbody>
                                <tr>
                                    <td>
                                        <i class=" fa fa-tasks"></i>
                                    </td>
                                    <td>New Task Issued</td>
                                    <td> 02</td>
                                </tr>
                                <tr>
                                    <td>
                                        <i class="fa fa-exclamation-triangle"></i>
                                    </td>
                                    <td>Task Pending</td>
                                    <td> 14</td>
                                </tr>
                                <tr>
                                    <td>
                                        <i class="fa fa-envelope"></i>
                                    </td>
                                    <td>Inbox</td>
