@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    <meta name="author" content="Mosaddek">
    <meta name="keyword" content="FlatLab, Dashboard, Bootstrap, Manager, Template, Theme, Responsive, Fluid, Retina">
    <meta http-equiv="Content-Security-Policy" content="upgrade-insecure-requests"> 

    <link rel="shortcut icon" href="http://thevectorlab.net/flatlab/img/favicon.png">

    <title>FlatLab - Flat & Responsive Bootstrap Manager Template</title>

    <!-- Bootstrap core CSS -->
    <link href="~/flatlab-master/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/flatlab-master/css/bootstrap-reset.css" rel="stylesheet">
    <!--external css-->
    <link href="~/flatlab-master/assets/font-awesome/css/font-awesome.css" rel="stylesheet"/>
    <link href="~/flatlab-master/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.css" rel="stylesheet" type="text/css" media="screen"/>
    <link rel="stylesheet" href="~/flatlab-master/css/owl.carousel.css" type="text/css">

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css" integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g==" crossorigin="anonymous" referrerpolicy="no-referrer"/>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css" integrity="sha256-eZrrJcwDc/3uDhsdt61sL2oOBY362qM3lon1gyExkL0=" crossorigin="anonymous"/>

    <!--right slidebar-->
    <link href="~/flatlab-master/css/slidebars.css" rel="stylesheet">

    <!-- Custom styles for this template -->

    <link href="~/flatlab-master/css/style.css" rel="stylesheet">
    <link href="~/flatlab-master/css/style-responsive.css" rel="stylesheet"/>


    <!-- HTML5 shim and Respond.js IE8 support of HTML5 tooltipss and media queries -->
    <!--[if lt IE 9]>
        <script src="~/flatlab-master/js/html5shiv.js"></script>
        <script src="~/flatlab-master/js/respond.min.js"></script>
    <![endif]-->
    <script src="~/Scripts/jquery-3.4.1.min.js"></script>
    <script src="~/Scripts/JqueryCustom/jquery-ui.js"></script>
    <link rel="stylesheet" href="~/Scripts/JqueryCustom/jquery-ui.theme.css">
</head>

<body>

<section id="container">
    <!--header start-->
    <header class="header white-bg">
        <div class="sidebar-toggle-box">
            <i class="fa fa-bars"></i>
        </div>
        <!--logo start-->
        <a href="/Manager" class="logo">Flat<span>lab</span></a>
        <!--logo end-->
        <div class="top-nav ">
            <!--search & user info start-->
            <ul class="nav pull-right top-menu">
                <li>
                    <input type="text" class="form-control search" placeholder="Search">
                </li>
                <!-- user login dropdown start-->
                <li class="dropdown">
                    <a data-toggle="dropdown" class="dropdown-toggle" href="~/flatlab-master/index.html#">
                        <img alt="" src="~/flatlab-master/img/avatar1_small.jpg">
                        <span class="username">@Session["User"]</span>
                        <b class="caret"></b>
                    </a>
                    <ul class="dropdown-menu extended logout">
                        <div class="log-arrow-up"></div>
                        <li>
                            <a href="/Manager/index#"><i class=" fa fa-suitcase"></i>Profile</a>
                        </li>
                        <li>
                            <a href="/Manager/index#"><i class="fa fa-cog"></i> Settings</a>
                        </li>
                        <li>
                            <a href="/Manager/index#"><i class="fa fa-bell-o"></i> Notification</a>
                        </li>
                        <li>
                            <a href="/login/logoutPersonel"><i class="fa fa-key"></i> Log Out</a>
                        </li>
                    </ul>
                </li>
                <!-- user login dropdown end -->
            </ul>
            <!--search & user info end-->
        </div>
    </header>
    <!--header end-->
    <!--sidebar start-->
    <aside>
        <div id="sidebar" class="nav-collapse ">
            <!-- sidebar menu start-->
            <ul class="sidebar-menu" id="nav-accordion">
                @if (User.IsInRole("Manager"))
                {
                    <li>
                        <a class="active" href="/Manager/index">
                            <i class="fa fa-dashboard"></i>
                            <span>Company</span>
                        </a>
                    </li>
                }

                <li class="sub-menu">
                    <a href="javascript:;">
                        <i class="fa fa-laptop"></i>
                        <span>Personel</span>
                    </a>
                </li>

                <li class="sub-menu">
                    <a href="javascript:;">
                        <i class="fa fa-book"></i>
                        <span>Survey</span>
                    </a>
                </li>

                <li class="sub-menu">
                    <a href="javascript:;">
                        <i class=" fa fa-bar-chart-o"></i>
                        <span>Charts</span>
                    </a>
                    <ul class="sub">
                        <li>
                            <a href="~/flatlab-master/morris.html">Morris</a>
                        </li>
                        <li>
                            <a href="~/flatlab-master/chartjs.html">Chartjs</a>
                        </li>
                        <li>
                            <a href="~/flatlab-master/flot_chart.html">Flot Charts</a>
                        </li>
                        <li>
                            <a href="~/flatlab-master/xchart.html">xChart</a>
                        </li>
                    </ul>
                </li>
                <li>
                    <a href="javascript:;">
                        <i class="fa fa-user-md"></i>
                        <span>Login Page</span>
                    </a>
                    <ul class="sub">
                        <li>
                            <a href="/Login/index">Admin Login</a>
                        </li>
                        <li>
                            <a href="/Login/UserLogin">User Login</a>
                        </li>
                    </ul>
                </li>

                <!--multi level menu start-->
                <li class="sub-menu">
                    <a href="javascript:;">
                        <i class="fa fa-sitemap"></i>
                        <span>Authorization</span>
                    </a>
                </li>
                <!--multi level menu end-->

            </ul>
            <!-- sidebar menu end-->
        </div>
    </aside>
    <!--sidebar end-->
    <!--main content start-->
    <section id="main-content">
        <section class="wrapper">
            @RenderBody()
        </section>
    </section>
    <footer class="site-footer">
        <div class="text-center">
            2013 &copy; FlatLab by VectorLab.
            <a href="~/flatlab-master/index.html#" class="go-top">
                <i class="fa fa-angle-up"></i>
            </a>
        </div>
    </footer>
    <!--footer end-->
</section>
<!-- js placed at the end of the document so the pages load faster -->
<script src="~/flatlab-master/js/jquery.js"></script>
<script src="~/flatlab-master/js/bootstrap.min.js"></script>
<script class="include" type="text/javascript" src="~/flatlab-master/js/jquery.dcjqaccordion.2.7.js"></script>
<script src="~/flatlab-master/js/jquery.scrollTo.min.js"></script>
<script src="~/flatlab-master/js/jquery.nicescroll.js" type="text/javascript"></script>
<script src="~/flatlab-master/js/jquery.sparkline.js" type="text/javascript"></script>
<script src="~/flatlab-master/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.js"></script>
<script src="~/flatlab-master/js/owl.carousel.js"></script>
<script src="~/flatlab-master/js/jquery.customSelect.min.js"></script>
<script src="~/flatlab-master/js/respond.min.js"></script>

<!--right slidebar-->
<script src="~/flatlab-master/js/slidebars.min.js"></script>

<!--common script for all pages-->
<script src="~/flatlab-master/js/common-scripts.js"></script>

<!--script for this page-->
<script src="~/flatlab-master/js/sparkline-chart.js"></script>
<script src="~/flatlab-master/js/easy-pie-chart.js"></script>
<script src="~/flatlab-master/js/count.js"></script>

<script>

        //owl carousel

        $(document).ready(function () {
            $("#owl-demo").owlCarousel({
                navigation: true,
                slideSpeed: 300,
                paginationSpeed: 400,
                singleItem: true,
                autoPlay: true

            });
        });

        //custom select box

        $(function () {
            $('select.styled').customSelect();
        });

        $(window).on("resize", function () {
            var owl = $("#owl-demo").data("owlCarousel");
            owl.reinit();
        });

    </script>

</body>
</html>